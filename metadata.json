{
  "name": "juniorsysadmin-ubuntuzilla",
  "version": "0.1.0",
  "author": "juniorsysadmin",
  "summary": "Module for installing the Ubuntuzilla repository",
  "license": "Apache-2.0",
  "source": "https://github.com/juniorsysadmin/puppet-ubuntuzilla.git",
  "project_page": "https://github.com/juniorsysadmin/puppet-ubuntuzilla",
  "issues_url": "https://github.com/juniorsysadmin/puppet-ubuntuzilla/issues",
  "tags": ["firefox", "ubuntuzilla"],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "10.04",
        "12.04",
        "14.04"
      ]
    }
  ],
  "dependencies": [
    {
      "name":"puppetlabs/apt",
      "version_requirement":">= 2.0.0 < 3.0.0"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=3.8.7 < 5.0.0"
    }
  ]
}
